{
  "name": "electron-auto-update-test",
  "version": "1.0.18",
  "description": "demo app to test auto-update for electron apps using electron-updater package",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "electron .",
    "release": "build",
    "pack": "electron-builder --dir",
    "dist": "electron-builder -w",
    "publish": "build -p always"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Rdhaliwal97/electron-auto-update-test.git"
  },
  "build": {
    "appId": "com.electron.testapp",
    "productName": "testApp",
    "target": "NSIS",
    "publish": [{
      "provider": "github",
      "owner": "rdhaliwal97",
      "repo": "appReleases"
    }],
    "copyright": "Copyright Â© 2019 rdhaliwal97",
    "nsis": {
      "allowToChangeInstallationDirectory": true,
      "oneClick": false
    }
  },
  "author": "Manpreet Singh <rdhaliwal97@outlook.com>",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Rdhaliwal97/electron-auto-update-test/issues"
  },
  "homepage": "https://github.com/Rdhaliwal97/electron-auto-update-test#readme",
  "devDependencies": {
    "electron": "^6.0.9",
    "electron-builder": "^21.2.0"
  },
  "dependencies": {
    "electron-updater": "^4.1.2"
  }
}
